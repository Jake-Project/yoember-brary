<h1>Contacts</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Email</th>
      <th>Message</th>
    </tr>
  </thead>
  <tbody>
  {{#each model as |contact|}}
    <tr>
      <th style="width: 30%">{{contact.email}}</th>
      <td>{{contact.message}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>
